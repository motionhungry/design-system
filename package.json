{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rimraf node_modules",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "plop": "plop",
    "prisma": "pnpm --filter=@motionhungry/data run prisma",
    "prepare": "husky install",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@motionhungry/style-policy": "next",
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "plop": "^3.1.2",
    "prettier": "^2.5.1",
    "rimraf": "^5.0.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "motionhungry",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "turbo run lint:fix"
    ]
  }
}
