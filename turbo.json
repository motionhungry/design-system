{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "persistent": true
    },
    "lint": {},
    "lint:fix": {},
    "storybook": {}
  }
}
